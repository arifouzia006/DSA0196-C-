#include <iostream>
using namespace std;
int main() {
    int arr[5] = {1, 2, 3, 4, 5}; 
    int *ptr = arr;               
    cout << "The elements of the array are: ";
    for(int i = 0; i < 5; i++) {
        cout << *(ptr + i) << " ";  
    }
    cout<<endl;
    cout<<ptr<<endl;
    return 0;
}
